package Practice;
public class Practice
{
	public static void main(String[] args) 
	{
		int a[]=new int[] {10,20,30,40,50};
		int[] b=new int[] {a.length-1};
		int i,j,m=2;
		for(i=0;i<a.length;i++)
		{
			if(i<m)
			{
				b[i]=a[i];
			}
			else if(i==m)
			{
				continue;
			}
			else 
			{
				b[i-1]=a[i];
			}
		}
		for(int c:b)
		{
			System.out.print(c+" ");
		}
	}
}
